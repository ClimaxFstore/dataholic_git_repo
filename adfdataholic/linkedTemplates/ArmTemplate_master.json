{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfdataholic"
		},
		"ABS_dataholic_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ABS_dataholic'"
		},
		"AzureSqlDatabase1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase1'"
		},
		"AzureSqlDataholic_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDataholic'"
		},
		"AzureSqlDatabase1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "serverdataholic.database.windows.net"
		},
		"AzureSqlDatabase1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sql_database"
		},
		"AzureSqlDatabase1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "dataholic_user"
		},
		"AzureSqlDataholic_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "serverdataholic.database.windows.net"
		},
		"AzureSqlDataholic_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sql_database"
		},
		"AzureSqlDataholic_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "dataholic_user"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfdataholic_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ABS_dataholic_connectionString": {
						"value": "[parameters('ABS_dataholic_connectionString')]"
					},
					"AzureSqlDatabase1_password": {
						"value": "[parameters('AzureSqlDatabase1_password')]"
					},
					"AzureSqlDataholic_password": {
						"value": "[parameters('AzureSqlDataholic_password')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_userName')]"
					},
					"AzureSqlDataholic_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDataholic_properties_typeProperties_server')]"
					},
					"AzureSqlDataholic_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDataholic_properties_typeProperties_database')]"
					},
					"AzureSqlDataholic_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDataholic_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}